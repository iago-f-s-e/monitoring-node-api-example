{
  "name": "monitoring-node-api-example",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/iago-f-s-e/monitoring-node-api-example.git",
  "author": "iago fagundes <iago.fagundes@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:src && yarn build:orm",
    "build:src": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build:orm": "babel ormconfig.ts --extensions \".js,.ts\" --out-dir . --copy-files --no-copy-ignored",
    "start": "yarn build && yarn start:prod",
    "start:prod": "node dist/index.js",
    "start:dev": "rm ormconfig.js && ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "typeorm:generate": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:run": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "typeorm:revert": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^16.11.10",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "cors": "^2.8.5",
    "dd-trace": "^1.5.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "winston": "^3.3.3"
  }
}
